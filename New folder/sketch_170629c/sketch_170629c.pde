void setup(){
  size(1000,1000);
}


void draw(){
  background(3,0,0);
//eye wholes
fill(255,255,255);
ellipse(200,200,200,300);
ellipse(600,300,200,300);

if(mouseX > 200);
  mouseX = 200;
  
//the actual eyes
fill(3,0,0);
ellipse(mouseX,mouseY,100,100);
ellipse(mouseX+400,mouseY,100,100);






}